.template-list{
    font-size: 30rpx; line-height: 1.5;
}
.template-item{
    position: relative;
    margin-bottom:1px;
    background-color: #fff;
    padding: 30rpx;
    margin-bottom: 1px;
}
.template-item__name{
    color: #333;
}
.template-item__time{
    color: #999;
    font-size: 26rpx;
}
.template-item__arrow{
    position: absolute; right: 30rpx; top: 50%;
    transform: translateY(-50%);
}