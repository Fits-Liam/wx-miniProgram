.animo-rotateX {
    top:50%;
    left:50%;
    transform:translate3d(-50%,-50%,0);
    animation:rotateX 1s ease-in-out infinite;
    color: #999;
}

@keyframes rotateX{
      0% { transform:translate3d(-50%,-50%,0) rotateY(0); }
    100% { transform:translate3d(-50%,-50%,0) rotateY(360deg); }    
}


.animo-bounce {
    top:50%;
    left:50%;
    transform:translate3d(-50%,-50%,0);
    animation:bounce 1s ease-in-out infinite;
    color: #999;
}

@keyframes bounce{
     0% { transform:translate3d(-50%,-50%,0) scale(0.9); }
    50% { transform:translate3d(-50%,-50%,0) scale(1.1); }
    100% { transform:translate3d(-50%,-50%,0) scale(0.9); }     
}

