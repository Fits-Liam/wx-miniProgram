@import '/app.wxss';

.fixBottomNav {
	position: fixed;
	padding: 10rpx 0;
	box-shadow: 0 -1px 2px -1px #dbdbdb;
	background:rgba(255,255,255,1);
}

.index_nav {
	margin: 0 auto 5rpx auto;
	position: relative;
}

.msg_tips{
	position: absolute;
	top:0;
	right:0;
	width:16rpx;
	height: 16rpx;
	margin: -4rpx -4rpx 0 0;
	border-radius: 50%;
	background-color: #df4d4d;
}

.index-item_default {
	color: #6B6B6B;
}

.index-item_active {
	font-weight: bold;
	color: #2CC3CE;
}

