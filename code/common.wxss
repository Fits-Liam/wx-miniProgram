/* = 模块
------------------------------------------------------------------------------------ */
    /* 导航 */
        .m-bottom-nav{
            height: 102rpx;
        }
    /* 导航 */
        .m-bottom-bar{
            height: 110rpx;
        }
        .m-bottom-bar__main{
            display: flex; align-items: center; justify-content: center;
            position: fixed; left: 0; right: 0; bottom: 0;
            height: 110rpx;
            background-color: #25A0D2;
            font-size: 36rpx; color: #fff;
        }
        .m-bottom-bar__icon{
            width: 48rpx; height: 48rpx;
            margin-right: 20rpx;
        }
        .m-bottom-bar__btn{
            position: relative;
            display: flex; align-items: center; justify-content: center; flex: 1;
            height: 110rpx;
            border-radius: 0;
            background-color: #25A0D2;
            color: #fff;
        }
        .m-bottom-bar__btn:first-child:before{
            content: '';
            position: absolute; right: -0; top: 10%;
            width: 1px; height: 80%;
            background-color: rgba(255,255,255,.65);
        }
        .m-bottom-bar__btn_gary{
            border-top: 1px solid #D2D2D2;
            background-color: #EBEBEB;
            color: #25A0D2;
        }
        .m-bottom-bar__btn_gary:first-child:before{
            display: none;
        }
    /* 徽标 */
        .m-badge{
            position: relative;
            display: inline-block;
            line-height: 1;
            vertical-align: middle;
        }
        .m-badge_dot{
            position: absolute; top: -10rpx; right: -10rpx;
            width: 20rpx; height: 20rpx;
            background: #E22323;
            border-radius: 100%; 
            z-index: 10;
        }
        .m-badge_num{
            position: absolute; top: 0; right: 0;
            min-width: 28rpx; height: 28rpx;
            padding: 3rpx;
            background: #E22323;
            border-radius: 28rpx; 
            font-size: 22rpx; color: #fff; text-align: center;
            z-index: 10;
        }
        .m-badge_text{
            display: inline-block;
            padding: 8rpx;
            border-radius: 4rpx;
            font-size: 24rpx; color: #fff;
        }

        .m-badge_success{
             background-color: #25A0D2;
        }
        .m-badge_fail{
             background-color: #B3B3B3;
        }
        .m-badge_info{
             background-color: #2CC3CE;
        }
        .m-badge_danger{
             background-color: #FE5757;
        }
    /* 文章列表 */
        .m-empty{
            position: relative; left: 50%;
            width: 750rpx;
            padding-top: 170rpx; padding-bottom: 80rpx; margin-left: -375rpx;
            background-color: #f2f2f2;
            text-align: center;
        }
        .m-empty__icon{
            width: 273rpx;
        }
        .m-empty__icon_def{
            height: 200rpx;
        }
        .m-empty__icon_assess{
            height: 249rpx;
        }
        .m-empty__text{
            margin-top: 40rpx;
            font-size: 28rpx; color: #999;
        }
    /* 文章列表 */
        .m-article-item{
            display: flex;
            padding: 30rpx;
            border-bottom: 1px solid #E5E5E5;
        }
        .m-article-item.is-checked{
            background-image: url('data:image/png;base64,');
            background-position: 670rpx 140rpx;
            background-repeat: no-repeat;
            background-size: 50rpx;
        }
        .m-article-item:last-child{
            border: 0;
        }
        .m-article-item__thumb{
            margin-right: 30rpx;
        }
        .m-article-item__img{
            width: 232rpx; height: 158rpx;
            border-radius: 8rpx;
            box-shadow: 0 0 0 1px #e7e7e7;
        }
        .m-article-item__content{
            flex: 1;
        }
        .m-article-item__title{
            padding-top: 10rpx;
            font-size: 32rpx; line-height: 44rpx;
            text-overflow: ellipsis; word-wrap: break-word; overflow: hidden;
            display: -webkit-box;
            -webkit-box-orient: vertical;
            -webkit-line-clamp: 2;
            font-weight: 700;
        }
        .m-article-item__meta{
            margin-top: 30rpx;
            font-size: 26rpx; line-height: 1; color: #999;
        }
        .m-article-item__tag{
            display: inline-block;
            margin-right: 35rpx;
        }
        .m-article-item__tag .ui-icon{
            margin-right: 15rpx;
        }
    /* 用户列表 */
        .m-contact-item{
            border-bottom: 1px solid #E5E5E5;
        }
        .m-contact-item:last-child{
            border: 0;
        }

        .m-contact{
            display: flex;
            width:100%;
            line-height: 1;
        }
        .m-contact__avatar{
            margin-right: 20rpx;
        }
        .m-contact__avatar .avatar {
            width: 145rpx; height: 145rpx;
            border-radius: 50%;
            vertical-align: top;
            will-change: transform;
            box-shadow: 0 0 0 1px #e7e7e7;
        }
        .m-contact__info{
            display: flex; flex-direction: column; justify-content: center; flex: 1;
        }
        .m-contact__name{
            font-size: 32rpx;
        }
        .m-contact__record{
            font-size: 30rpx; line-height: 44rpx;
        }
        .m-contact__row{
            margin-top: 13rpx;
            font-size: 26rpx;
        }
        .m-contact__type{
            margin-right: 10rpx;
        }
        .m-contact__meta{
            display: inline-block;
            font-size: 26rpx; color: #999;
        }
        .m-contact__meta:first-child{
            margin-right: 20rpx;
        }
        .m-contact__btn{
            display: inline-block;
            padding-right: 15rpx; margin-right: 15rpx;
            border-right: 1px solid #e5e5e5;
        }
        .m-contact__btn:last-child{
            padding: 0; margin-right: 0;
            border: 0;
        }
        .m-contact__status.is-offline{
            background-color: #a8a8a8;
        }
        .m-contact__action{
            display: flex; align-items: center;
            position: relative;
        }
        .m-contact__tips{
            display: inline-block;
            padding: 10rpx 24rpx; margin-right: 40rpx;
            background-color: #FE5757;
            border-radius: 4rpx;
            font-size: 24rpx; color: #fff;
        }
        .m-contact__remark{
            margin-right: 40rpx;
            font-size: 28rpx;
        }
        .m-contact__arrow{
            position: absolute; top: 50%; right: 0;
            transform: translate3d(0,-50%,0);
        }
    /* 标签切换 */
        .m-tabs-mask{
            position: fixed; top: 0; right: 0; bottom: 0; left: 0; z-index: 99;
            width:100%; height:100%;
            background-color:rgba(0, 0, 0, 0.5);
        }
        .m-tabs{
            display: flex;
            position: relative; z-index: 1000;
            padding: 0 30rpx;
            background-color: #fff;
        }
        .m-tabs_between .m-tabs__scroll{
            display: flex; justify-content: space-between;
        }
        .m-tabs__average{
            display: flex; flex: 1;
            border-bottom: 1px solid #E5E5E5;
        }
        .m-tabs__average .m-tabs__tab{
            flex: 1;
        }
        .m-tabs__scroll{
            flex: 1;
            width: 100%; height: 100%;
            border-bottom: 1px solid #E5E5E5;
            font-size: 34rpx;
            overflow-x: hidden; white-space: nowrap;
        }
        .m-tabs__tab{
            display: inline-block;
            position: relative;
            padding: 20rpx 26rpx;
            min-height: 80rpx;
            font-size: 34rpx; line-height: 1; color: #6B6B6B; text-align:center;
        }
        .m-tabs__tab:before{
            content: '';
            position: absolute; right: 0; bottom: 0; left: 0;
            height: 4rpx;
            border-radius: 2rpx;
            background-color: #2CC3CE;
            transform: scaleX(0);
            transition: .1s;
        }
        .m-tabs__filter{
            position: relative; z-index: 1;
            width: 64rpx; height: 82rpx;
            background-color: #fff;
            background-position: right center;
            border-bottom: 1px solid #E5E5E5;
        }
        .m-tabs__filter:before{
            position: absolute; left: -19rpx; top: 0;
            content: '';
            width: 20rpx; height: 70rpx;
            background: linear-gradient(90deg, rgba(255,255,255,0), #fff);
        }
        .m-tabs__tab.is-active{
            color: #2CC3CE; font-weight: 700;
        }
        .m-tabs__tab.is-active:before {
            transition-timing-function: cubic-bezier(.4,0,.2,1);
            transform: scaleX(1);
            transition-duration: .3s;
        }
        .m-tabs__dropdown {
            position: absolute; left: 0; top: 82rpx; z-index: 2000;
            padding: 0 30rpx;
            box-sizing:border-box;
        }
        .m-tabs__srcoll {
            position: absolute; left: 0; top: 82rpx; z-index: 2000;
            max-height: 1100rpx;
            background-color: #fff;
        }
    /* 滚动加载提示 */
        .m-loading__icon {
            display: inline-block;
            width: 26rpx; height: 26rpx;
            margin-right: 5px;
            animation: loading-spinner 1s infinite linear;
            background-image: url('data:image/png;base64,');
            background-repeat: no-repeat;
            background-size: 100%;
            vertical-align: middle;
        }
        @keyframes loading-spinner{
            0%{ transform:rotate(0deg); }
            100%{ transform:rotate(360deg); }
        }
        .m-loading{
            display: flex; align-items: center;
            padding: 30rpx 148rpx;
        }
        .m-loading__line{
            display: flex; flex-grow: 1;
            height: 1px;
            background: #D1D1D1;
        }
        .m-loading__text{
            display: flex; align-items: center; justify-content: center;
            min-width: 120rpx;
            font-size: 26rpx; color: #999;
            padding: 0 20rpx;
        }